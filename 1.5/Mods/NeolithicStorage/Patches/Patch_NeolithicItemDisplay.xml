<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== ASNeolithicNeolithicItemDisplay ============================= -->

	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- ==================== Research Tab ============================= -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/tab</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/tab</xpath>
					<value>
						<tab>Main</tab>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]</xpath>
					<value>
						<tab>Main</tab>
					</value>
				</nomatch>
			</li>
			<!-- ==================== Horizontal Position ==================== -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/researchViewX</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/researchViewX</xpath>
					<value>
						<researchViewX>researchViewX</researchViewX>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]</xpath>
					<value>
						<researchViewX>researchViewX</researchViewX>
					</value>
				</nomatch>
			</li>
			<!-- ==================== Vertical Position ====================== -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/researchViewY</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]/researchViewY</xpath>
					<value>
						<researchViewY>researchViewY</researchViewY>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="ASNeolithicNeolithicItemDisplay"]</xpath>
					<value>
						<researchViewY>researchViewY</researchViewY>
					</value>
				</nomatch>
			</li>
		</operations>
	</Operation>

</Patch>