<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ResearchProjectDef[defName="BotchJob_ProfanedBasics"]</xpath>
		<value>
			<tab>Sigma_Medieval</tab>
		</value>
	</Operation>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="BotchJob_ProfanedBasics"]/techLevel</xpath>
		<value>
			<techLevel>Medieval</techLevel>
		</value>
	</Operation>

	<!-- ==================== Horizontal Position ==================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="BotchJob_ProfanedBasics"]/researchViewX</xpath>
		<value>
			<researchViewX>4</researchViewX>
		</value>
	</Operation>

	<!-- ==================== Vertical Position ====================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="BotchJob_ProfanedBasics"]/researchViewY</xpath>
		<value>
			<researchViewY>0.5</researchViewY>
		</value>
	</Operation>

</Patch>