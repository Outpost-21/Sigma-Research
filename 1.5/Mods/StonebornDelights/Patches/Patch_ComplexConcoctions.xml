<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="SD_ProcessedDrugs"]/tab</xpath>
		<value>
			<tab>Sigma_Medieval</tab>
		</value>
	</Operation>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="SD_ProcessedDrugs"]/techLevel</xpath>
		<value>
			<techLevel>Medieval</techLevel>
		</value>
	</Operation>

	<!-- ==================== Horizontal Position ==================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="SD_ProcessedDrugs"]/researchViewX</xpath>
		<value>
			<researchViewX>4</researchViewX>
		</value>
	</Operation>

	<!-- ==================== Vertical Position ====================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="SD_ProcessedDrugs"]/researchViewY</xpath>
		<value>
			<researchViewY>1.5</researchViewY>
		</value>
	</Operation>

</Patch>