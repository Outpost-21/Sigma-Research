<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== HighMechtech ============================= -->

	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- ==================== Research Tab ============================= -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/tab</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/tab</xpath>
					<value>
						<tab>Sigma_Spacer</tab>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]</xpath>
					<value>
						<tab>Sigma_Spacer</tab>
					</value>
				</nomatch>
			</li>
			<!-- ==================== Tech Level ============================= -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/techLevel</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/techLevel</xpath>
					<value>
						<techLevel>Spacer</techLevel>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]</xpath>
					<value>
						<techLevel>Spacer</techLevel>
					</value>
				</nomatch>
			</li>
			<!-- ==================== Horizontal Position ==================== -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/researchViewX</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/researchViewX</xpath>
					<value>
						<researchViewX>8</researchViewX>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]</xpath>
					<value>
						<researchViewX>8</researchViewX>
					</value>
				</nomatch>
			</li>
			<!-- ==================== Vertical Position ====================== -->
			<li Class="PatchOperationConditional">
				<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/researchViewY</xpath>
				<match Class="PatchOperationReplace">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]/researchViewY</xpath>
					<value>
						<researchViewY>4.5</researchViewY>
					</value>
				</match>
				<nomatch Class="PatchOperationAdd">
					<xpath>/Defs/ResearchProjectDef[defName="HighMechtech"]</xpath>
					<value>
						<researchViewY>4.5</researchViewY>
					</value>
				</nomatch>
			</li>
		</operations>
	</Operation>

</Patch>