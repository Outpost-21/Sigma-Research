<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ResearchProjectDef[defName="Outland_KalociteFarming"]</xpath>
		<value>
			<tab>Main</tab>
		</value>
	</Operation>

	<!-- ==================== Horizontal Position ==================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="Outland_KalociteFarming"]/researchViewX</xpath>
		<value>
			<researchViewX>4</researchViewX>
		</value>
	</Operation>

	<!-- ==================== Vertical Position ====================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="Outland_KalociteFarming"]/researchViewY</xpath>
		<value>
			<researchViewY>0</researchViewY>
		</value>
	</Operation>

</Patch>