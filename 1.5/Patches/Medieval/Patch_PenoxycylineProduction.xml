<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ResearchProjectDef[defName="PenoxycylineProduction"]</xpath>
		<value>
			<tab>Sigma_Medieval</tab>
		</value>
	</Operation>

	<!-- ==================== Tech Level ============================= -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="PenoxycylineProduction"]/techLevel</xpath>
		<value>
			<techLevel>Medieval</techLevel>
		</value>
	</Operation>

	<!-- ==================== Horizontal Position ==================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="PenoxycylineProduction"]/researchViewX</xpath>
		<value>
			<researchViewX>6</researchViewX>
		</value>
	</Operation>

	<!-- ==================== Vertical Position ====================== -->

	<Operation Class="PatchOperationReplace">
		<xpath>/Defs/ResearchProjectDef[defName="PenoxycylineProduction"]/researchViewY</xpath>
		<value>
			<researchViewY>3</researchViewY>
		</value>
	</Operation>

</Patch>